extends layout

block content
	h1 Login
	p Please login below
	form(method='post',action='/users/login',enctype='multipart/form-data')
		.form-group
			label Username
			input.form-control(name='user', type='text')
		.form-group
			label Password
			input.form-control(name='pwd', type='password')
		input.btn.btn-default(name='submit', type='submit',value='Login')